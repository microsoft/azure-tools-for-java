      <reference ref="AzureToolkit.AzureSignIn"/>
      <separator/>
      <reference ref="AzureToolkit.ValidateCertificate"/>
      <separator/>
      <reference ref="AzureToolkit.OpenSdkReferenceBook"/>
      <separator/>
